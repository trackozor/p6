{
  "name": "p6",
  "version": "1.0.0",
  "description": "Photographers Directory permet de rechercher et contacter des photographes talentueux pour des projets professionnels et personnels.",
  "main": "eslint.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm-run-all --parallel server sass",
    "server": "json-server --watch db.json --port 3001",
    "sass": "sass --watch scss/pages-scss/index-main.scss:css/main.css scss/pages-scss/photographer-main.scss:css/pages/photographer.css",
    "test": "jest",
    "lint": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": ["photography", "directory", "photographers"],
  "author": "Ton Nom",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "json-server": "^1.0.0-beta.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.83.4",
    "prettier": "^3.1.0"
  }
}

