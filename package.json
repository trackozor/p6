{
  "name": "p6",
  "version": "1.0.0",
  "description": "Photographers Directory permet de rechercher et contacter des photographes talentueux pour des projets professionnels et personnels.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel server sass",
    "server": "json-server --watch db.json --port 3001",
    "sass": "sass --watch scss/pages-scss/index-main.scss:css/index-main.css scss/pages-scss/photographer-main.scss:css/photographer-main.css",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --testMatch '**/e2e/**'",
    "lint": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "photography",
    "directory",
    "photographers"
  ],
  "author": "Ton Nom",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "json-server": "^1.0.0-beta.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "sass": "^1.83.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "glob": "^11.0.1",
    "lru-cache": "^11.0.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trackozor/p6.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/trackozor/p6/issues"
  },
  "homepage": "https://github.com/trackozor/p6#readme"
}
