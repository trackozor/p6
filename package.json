{
  "name": "p6",
  "version": "1.0.0",
  "description": "Projet Fisheye - Frontend avec SASS, Jest, et ESLint",
  "main": "main.js",
  "scripts": {
    "start": "npm-run-all --parallel build:sass test:watch",
    "dev": "npm run backend & npm run start",
    "backend": "json-server --watch mock-backend/db.json --port 3000",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "lint:folder": "eslint ./js/**/*.js --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build:sass": "sass scss:css --style compressed",
    "sass": "sass --watch scss:./css scss/pages-scss/photographer-main.scss:./css/pages/photographer.css"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.18.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.83.0",
    "stylelint": "^16.14.1",
    "stylelint-scss": "^5.0.0"
  },
  "type": "module"
}
